{
  "name": "NuxtTemplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:docker": "docker-compose up",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lint:fix": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "test": "jest --config jest.config.js ./src/test/",
    "test:e2e": "jest --config jest-e2e.config.js --runInBand ./test.e2e/",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.8.3",
    "nuxt": "^2.15.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nuxt/types": "^2.15.2",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "expect-playwright": "^0.3.2",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.5.0",
    "lint-staged": "^10.5.4",
    "playwright": "^1.9.1",
    "playwright-firefox": "^1.9.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2",
    "vue-jest": "^3.0.4"
  }
}
